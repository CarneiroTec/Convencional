Fixo Arquivo    *ctec_convencional_arquivo_abra(Imutável Caractere *nomeArquivo, Imutável Caractere *modo) { Retorne fopen(nomeArquivo, modo); }
Fixo Arquivo    *ctec_convencional_arquivo_temporario(Vazio) { Retorne tmpfile(); }
Fixo Arquivo    *ctec_convencional_arquivo_crie(Imutável Caractere *nomeArquivo, Imutável Caractere *modo) { Retorne fopen(nomeArquivo, modo); }
Fixo Caractere  *ctec_convencional_arquivo_nome_temporario(Caractere *s) { Retorne tmpnam(s); }
Fixo Caractere  *ctec_convencional_arquivo_obtenha_linha(Caractere *s, Inteiro n, Arquivo *arquivo) { Retorne fgets(s, n, arquivo); }
Fixo Vazio      ctec_convencional_arquivo_configure_reserva(Arquivo *arquivo, Caractere *buf) { setbuf(arquivo, buf); }
Fixo Inteiro    ctec_convencional_arquivo_defina_reserva(Arquivo *arquivo, Caractere *buf, Inteiro modo, Inteiro tamanho) { Retorne setvbuf(arquivo, buf, modo, tamanho); }
Fixo Inteiro    ctec_convencional_arquivo_despeje(Arquivo *arquivo) { Retorne fflush(arquivo); }
Fixo Inteiro    ctec_convencional_arquivo_devolva_caractere(Inteiro c, Arquivo *arquivo) { Retorne ungetc(c, arquivo); }
Fixo Inteiro    ctec_convencional_arquivo_escreva(Imutável Vazio *ponteiro, Inteiro tamanho, Inteiro quantidade, Arquivo *arquivo) { Retorne fwrite(ponteiro, tamanho, quantidade, arquivo); }
Fixo Inteiro    ctec_convencional_arquivo_erro(Arquivo *arquivo) { Retorne ferror(arquivo); }
Fixo Inteiro    ctec_convencional_arquivo_exiba_caractere(Inteiro c, Arquivo *arquivo) { Retorne fputc(c, arquivo); }
Fixo Inteiro    ctec_convencional_arquivo_exiba_linha(Imutável Caractere *s, Arquivo *arquivo) { Retorne fputs(s, arquivo); }
Fixo Inteiro    ctec_convencional_arquivo_feche(Arquivo *arquivo) { Retorne fclose(arquivo); }
Fixo Inteiro    ctec_convencional_arquivo_fim(Arquivo *arquivo) { Retorne feof(arquivo); }
Fixo Inteiro    ctec_convencional_arquivo_leia(Vazio *ponteiro, Inteiro tamanho, Inteiro quantidade, Arquivo *arquivo) { Retorne fread(ponteiro, tamanho, quantidade, arquivo); }
Fixo Vazio      ctec_convencional_arquivo_limpe_erro(Arquivo *arquivo) { clearerr(arquivo); }
Fixo Inteiro    ctec_convencional_arquivo_obtenha_caractere(Arquivo *arquivo) { Retorne fgetc(arquivo); }
Fixo Longo      ctec_convencional_arquivo_obtenha_posicao(Arquivo *arquivo) { Retorne ftell(arquivo); }
Fixo Inteiro    ctec_convencional_arquivo_posicione(Arquivo *arquivo, Longo deslocamento, Inteiro origem) { Retorne fseek(arquivo, deslocamento, origem); }
Fixo Vazio      ctec_convencional_arquivo_reinicie(Arquivo *arquivo) { rewind(arquivo); }
Fixo Inteiro    ctec_convencional_arquivo_remova(Imutável Caractere *nomeArquivo) { Retorne remove(nomeArquivo); }
Fixo Inteiro    ctec_convencional_arquivo_renomeie(Imutável Caractere *antigo, Imutável Caractere *novo) { Retorne rename(antigo, novo); }

